{
  "name": "@elastic/react-search-components",
  "version": "0.1.0",
  "description": "A collection of React UI components for building search experiences",
  "author": "Jason Stoltzfus <jastoltz24@gmail.com>",
  "homepage": "https://github.com/elastic/search-ui#readme",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elastic/search-ui.git"
  },
  "scripts": {
    "test": "jest",
    "build-css": "node-sass-chokidar src/ -o lib/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build-js": "babel src --out-dir lib --ignore '**/*.test.js'",
    "build": "npm-run-all build-css build-js",
    "prepare": "npm run build"
  },
  "bugs": {
    "url": "https://github.com/elastic/search-ui/issues"
  },
  "peerDependencies": {
    "react": "^16.6.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/preset-react": "^7.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "jest": "^23.6.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "dependencies": {
    "react-select": "^2.1.1"
  }
}
